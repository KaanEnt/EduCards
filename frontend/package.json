{
  "name": "flashcards-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:react\" \"npm run start:backend\"",
    "start:react": "react-scripts start",
    "start:backend": "cd ../server && poetry run python manage.py runserver",
    "setup": "npm install && cd .. && python -m venv venv && .\\venv\\Scripts\\pip install -r requirements.txt && cd server && ..\\venv\\Scripts\\python manage.py migrate",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.3",
    "@mui/icons-material": "^5.15.3",
    "@mui/material": "^5.15.3",
    "@react-spring/web": "^9.7.5",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.5",
    "concurrently": "^9.1.0",
    "leva": "^0.9.35",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "react-spring": "^9.7.3",
    "react-use-gesture": "^9.1.3",
    "web-vitals": "^2.1.4",
    "zustand": "^4.4.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
